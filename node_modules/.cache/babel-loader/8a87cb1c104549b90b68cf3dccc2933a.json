AA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,uBAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAjBA;AAkBA,IAAA,cAlBA,0BAkBA,OAlBA,EAkBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,UAAA,CAAA,2BAAA,OAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,UAAA;;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AA1BA;AAzBA,CAAA","sourcesContent":["<template>\n<div class=\"container\">\n  <div class=\"menu\">\n    <p><a><i class=\"container\"></i></a></p>\n    <h4>{{user.firstName}} {{user.lastName}} <a @click=\"logout\"><i class=\"menu-item\"> logout</i></a></h4>\n  </div>\n\n  <div class=\"row\">\n  <div class=\"container\" style=\"background-color:lightblue;\">\n    <div class=\"header\" >Welcome {{user.firstName}} {{user.lastName}}!</div>\n    <img src=\"/images/icon2.png\" style=\"width: 25%\" class=\"profilepic\">\n    <p></p>\n    <h3>You're following...</h3>\n      <table class=\"table\">\n        <tbody>\n          <tr v-for=\"profile in friends\" v-bind:key=\"profile._id\">\n            <th scope=\"row\">@{{profile.username}}</th>\n            <td>{{profile.name}}</td>\n            <td>{{profile.email}}</td>\n            <td>{{profile.city}}</td>\n          <button class=\"auto\" v-on:click= \"unfollowFriend(profile)\">Unfollow</button>\n          </tr>\n        </tbody>\n\n      </table>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'MyProfile',\n  data() {\n    return {\n      username: \"\",\n      name: \"\",\n      email: \"\",\n      city: \"\",\n    }\n  },\n  computed: {\n    profiles () {\n      return this.$root.$data.profiles;\n    },\n    user() {\n      return this.$root.$data.user;\n    },\n    friends() {\n      return this.$root.$data.friends;\n    },\n  },\n  async created () {\n    this.getFriends();\n  },\n\n  methods: {\n    async logout() {\n      try {\n        await axios.delete(\"/api/users\");\n        this.$root.$data.user = null;\n      } catch (error) {\n        this.$root.$data.user = null;\n      }\n    },\n    async getFriends() {\n  try {\n    let response = await axios.get(\"/api/profiles/friends\");\n    this.$root.$data.friends = response.data;\n    return true;\n    } catch (error) {\n      console.log(error);\n    }\n  },\n  async unfollowFriend(profile) {\n    try {\n      await axios.delete(\"/api/profiles/friends/\" + profile._id);\n      this.getFriends();\n      return true;\n    } catch (error) {\n      console.log(error);\n    }\n  },\n}\n}\n</script>\n\n<style scoped>\n.friends{\n  justify-content: center;\n\n}\n.menu-item {\n  display: flex;\n  flex-direction: column;\n}\n\n.menu-item p {\n  margin: 0px;\n}\n.menu {\n  display: flex;\n  justify-content: space-between;\n}\n\n.menu h2 {\n  font-size: 14px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}